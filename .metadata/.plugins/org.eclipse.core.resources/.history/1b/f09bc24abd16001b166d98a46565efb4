package no08;
import java.util.Scanner;
public class no8 {

	public static void main(String[] args) {
		var console=new Scanner(System.in);
		System.out.printf("請輸入您要買的書本數量:");
		int BookNo=console.nextInt();
		console.close();
		if (BookNo<=0) {
			System.out.println("數量錯誤,程式結束!");
			return;
		}
			
			

	}

}
