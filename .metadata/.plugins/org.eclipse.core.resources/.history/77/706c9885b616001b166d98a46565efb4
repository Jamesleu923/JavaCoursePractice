package no05;

public class no5 {

	public static void main(String[] args) {
		
		System.out.println("¥Îfor°j°é");	
		for (int i=2; i<10;i++) {
			for (int j=1;j<10;j++) {
				System.out.printf("%d * %d =%d   ",i,j,i*j);
			}
			System.out.println();
		}

		System.out.println("¥Îdo ..while°j°é");			
		

	}

}
