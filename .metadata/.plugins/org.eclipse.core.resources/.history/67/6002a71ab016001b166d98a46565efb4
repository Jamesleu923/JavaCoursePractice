package no03;
//要引入下方套件,才可以由使用者輸入.
import java.util.Scanner;
//
public class no03 {

	public static void main(String[] args) {
		String[] vstr=new String(2);
		var console=new Scanner(System.in);
		System.out.println("請輸入二組字串:");
		
		for (int i=0; i<2 ;i++) {
			System.out.printf("第%d個字串:", i+1);
			
			
		}
	}

}
